# Azure DevOps settings
AZDO_ORG_URL=https://dev.azure.com/<your-org>/
AZDO_PROJECT=<your-project-name>
AZDO_WIKI=<your-wiki-name>.wiki
AZDO_PAT=<your-azdo-personal-access-token>
OUTPUT_DIR=./wiki-export

# Azure AI Search settings
RESOURCE_GROUP="<your-resource-group>"
SEARCH_SERVICE_NAME="<your-search-service-name>"
API_VERSION="2023-11-01"
AI_SEARCH_KEY="<your-search-admin-or-query-key>"
SCHEMA_FILE="it_knowledge.json"

# Azure AI Foundry settings
PROJECT_ENDPOINT=https://<your-resource-name>.services.ai.azure.com/api/projects/<your-project-name>
AI_PROJECT_KEY=<your-ai-project-key>
MODEL_DEPLOYMENT_NAME="<your-model-deployment-name>"

# Azure OpenAI Service
AZURE_OPENAI_ENDPOINT="https://<your-azure-openai-resource>.openai.azure.com"
AZURE_OPENAI_API_KEY="<your-azure-openai-key>"
AZURE_OPENAI_API_VERSION="2025-01-01-preview"
AZURE_OPENAI_CHAT_DEPLOYMENT_NAME="<your-chat-deployment>"
AZURE_OPENAI_EMBEDDING_MODEL="text-embedding-3-large"
AZURE_OPENAI_EMBEDDING_DEPLOYMENT="<your-embedding-deployment>"

# NOTE: This is a sample .env file. Replace placeholders with your values.
# Do NOT commit real secrets or keys to version control.